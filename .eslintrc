{
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "space-before-blocks": 2,
    "space-after-keywords": 2,
    "no-unreachable": 2,
    "yoda": [2, "never"],
    "eqeqeq": 2,
    "space-in-parens": [2, "never"]
  },
  "env": {
    "node": true
  },
  "extends": "eslint:recommended"
}